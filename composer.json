{
    "name": "stic/proxypay-php",
    "type": "library",
    "description": "ProxyPay implementation for PHP",
    "keywords": ["php","proxypay"],
    "homepage": "https://sticnegocios.com",
    "license": "MIT",
    "version": "1.0.1",
    "authors": [
        {
            "name": "Joao Nzango",
            "email": "joaonzango@gmail.com",
            "homepage": "http://stic.guthub.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "doetech/diedump": "^1.2",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "stic\\ProxyPayPHP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stic\\ProxyPayPHP\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
